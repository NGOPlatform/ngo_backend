package ngoplatform.ngo.service;

import ngoplatform.ngo.model.ONG;
import ngoplatform.ngo.repository.ONGRepository;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service
public class ONGService {
    
    @Autowired
    private ONGRepository repository;
    public List<ONG> getSomeONGs() {
        //Return only the first 10 ONGs
        return repository.findAll().subList(0, 10);
    }
}
